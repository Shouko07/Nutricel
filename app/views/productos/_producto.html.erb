<div id="<%= dom_id producto %>">
  <p>
    <strong>Id producto:</strong>
    <%= producto.id_producto %>
  </p>

  <p>
    <strong>Nombre:</strong>
    <%= producto.nombre %>
  </p>

  <p>
    <strong>Precio:</strong>
    <%= producto.precio %>
  </p>

  <p>
    <strong>Peso:</strong>
    <%= producto.peso %>
  </p>

  <p>
    <strong>Imagen:</strong>
    <%= producto.imagen %>
  </p>

  <p>
    <strong>Fecha expiracion:</strong>
    <%= producto.fecha_expiracion %>
  </p>

</div>
